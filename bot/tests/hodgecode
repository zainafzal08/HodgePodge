!hp do 1/2 -> 0.5
!hp do 1/3 -> 0.33333333
!hp do 1.0/3 -> 0.33333333
!hp do 0 -> 0
!hp do 1.0 -> 1
!hp do 3 -> 3
!hp do 100+0 -> 100
